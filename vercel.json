{
  "buildCommand": "cd bounsser-frontend && npm run build",
  "outputDirectory": "bounsser-frontend/.next",
  "installCommand": "npm install && cd bounsser-frontend && npm install",
  "devCommand": "cd bounsser-frontend && npm run dev",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./bounsser-frontend"
}
